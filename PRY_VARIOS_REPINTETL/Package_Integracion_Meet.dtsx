                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  FEC_CREACION,
        COD_LINEA_NEGOCIO,
        GUID_CRM_ALUM,
        SEC_ALUMNO,
        COD_ALUMNO,
        DES_MODALIDAD_ESTUDIO,
        COD_PRODUCTO,
        DES_CARRERA,
        NUM_CICLO_ALUMNO,
        DES_LOCAL        
FROM (
        
        SELECT A.FECHA_CREACION AS FEC_CREACION,
               A.COD_LINEA_NEGOCIO,
               A.GUID_CRM_ALUM,
               A.SEC_ALUM AS SEC_ALUMNO,
               T.COD_ALUMNO,
               ME.NOMBRE AS DES_MODALIDAD_ESTUDIO,
               T.COD_PRODUC_MAT AS COD_PRODUCTO,
               PR.DESCRIPCION AS DES_CARRERA, 
               T.CICLO_ALUMNO AS NUM_CICLO_ALUMNO,
               L.DESCRIPCION AS DES_LOCAL,
               ROW_NUMBER() OVER (PARTITION BY T.COD_ALUMNO ORDER BY ID DESC) AS MAXIMA_MAT
            FROM WSACTDATOS.AD_ALUMNO    A,
                 MATRICULA               T,
                 MODALIDAD_ESTUD         ME,
                 PRODUCTO                PR,
                 LOCAL                   L         
           WHERE A.COD_LINEA_NEGOCIO = 'U' 
             AND SUBSTR(A.USUARIO_CREACION,2,10) = T.COD_ALUMNO
             AND T.COD_LINEA_NEGOCIO = PR.COD_LINEA_NEGOCIO
             AND T.COD_PRODUC_MAT = PR.COD_PRODUCTO
             AND PR.ES_CARRERA = 'SI'
             AND T.COD_LINEA_NEGOCIO = ME.COD_LINEA_NEGOCIO(+)
             AND T.COD_MODAL_EST = ME.COD_MODAL_EST(+)                  
             AND T.COD_LOCAL = L.COD_LOCAL(+)          
             AND T.COD_LINEA_NEGOCIO = 'U'
             AND NVL(T.COD_MODAL_EST, 'AC') IN ('AC', 'FC')

UNION
        SELECT A.FECHA_CREACION AS FEC_CREACION,
               A.COD_LINEA_NEGOCIO,
               A.GUID_CRM_ALUM,
               A.SEC_ALUM AS SEC_ALUMNO,
               T.COD_ALUMNO,
               ME.NOMBRE AS DES_MODALIDAD_ESTUDIO,
               T.COD_PRODUC_MAT AS COD_PRODUCTO,
               PR.DESCRIPCION AS DES_CARRERA, 
               T.CICLO_ALUMNO AS NUM_CICLO_ALUMNO,
               L.DESCRIPCION AS DES_LOCAL,
               ROW_NUMBER() OVER (PARTITION BY T.COD_ALUMNO ORDER BY ID DESC) AS MAXIMA_MAT
            FROM WSACTDATOS.AD_ALUMNO            A,
                 MATRICULA@PRODSI2               T,
                 MODALIDAD_ESTUD@PRODSI2         ME,
                 PRODUCTO@PRODSI2                PR,
                 LOCAL@PRODSI2                   L         
           WHERE A.COD_LINEA_NEGOCIO = 'E' 
             AND SUBSTR(A.USUARIO_CREACION,2,10) = T.COD_ALUMNO
             AND T.COD_LINEA_NEGOCIO = PR.COD_LINEA_NEGOCIO
             AND T.COD_PRODUC_MAT = PR.COD_PRODUCTO
             AND PR.ES_CARRERA = 'SI'
             AND T.COD_LINEA_NEGOCIO = ME.COD_LINEA_NEGOCIO(+)
             AND T.COD_MODAL_EST = ME.COD_MODAL_EST(+)                  
             AND T.COD_LOCAL = L.COD_LOCAL(+)          
             AND T.COD_LINEA_NEGOCIO = 'E'
             
)
WHERE MAXIMA_MAT = 1</property>
