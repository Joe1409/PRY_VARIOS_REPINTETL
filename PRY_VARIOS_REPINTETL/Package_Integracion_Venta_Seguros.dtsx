    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{13F70041-230C-4E34-936E-79C8FC6E0D1F}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;&#xA;Select x.ContactId, x.FirstName, x.MiddleName, x.LastName, x.BirthDate, x.Genero, &#xA;&#x9;&#x9;x.CRM_TipoDocumentoName, x.CRM_NroDocumento, x.Telephone1, x.Telephone2, x.Telephone3, &#xA;&#x9;&#x9;x.EmailAddress1, x.EmailAddress2, x.EmailAddress3, &#xA;&#x9;&#x9;x.Tipo_Apoderado, x.FullName_Apoderado, x.CRM_TipoDocumentoName_Apoderado, x.CRM_NroDocumento_Apoderado&#xA;From (&#xA;&#x9;&#x9;Select c.ContactId, &#xA;&#x9;&#x9;&#x9;&#x9;c.FirstName, &#xA;&#x9;&#x9;&#x9;&#x9;c.MiddleName, &#xA;&#x9;&#x9;&#x9;&#x9;c.LastName, &#xA;&#x9;&#x9;&#x9;&#x9;c.BirthDate, &#xA;&#x9;&#x9;&#x9;&#x9;SUBSTRING(sm.VALUE,1,50) As Genero,&#xA;&#x9;&#x9;&#x9;&#x9;c.CRM_TIPODOCUMENTONAME, &#xA;&#x9;&#x9;&#x9;&#x9;c.CRM_NRODOCUMENTO, &#xA;&#x9;&#x9;&#x9;&#x9;c.TELEPHONE1, &#xA;&#x9;&#x9;&#x9;&#x9;c.TELEPHONE2, &#xA;&#x9;&#x9;&#x9;&#x9;c.TELEPHONE3,&#xA;&#x9;&#x9;&#x9;&#x9;c.EMAILADDRESS1, &#xA;&#x9;&#x9;&#x9;&#x9;c.EMAILADDRESS2, &#xA;&#x9;&#x9;&#x9;&#x9;c.EMAILADDRESS3,&#xA;&#x9;&#x9;&#x9;&#x9;cn.Record2RoleIdName As Tipo_Apoderado,&#xA;&#x9;&#x9;&#x9;&#x9;SUBSTRING(cn.Record2IdName,1,500) As FullName_Apoderado,&#xA;&#x9;&#x9;&#x9;&#x9;c2.CRM_TIPODOCUMENTONAME As CRM_TIPODOCUMENTONAME_APODERADO,&#xA;&#x9;&#x9;&#x9;&#x9;c2.CRM_NRODOCUMENTO As CRM_NRODOCUMENTO_APODERADO,&#xA;&#x9;&#x9;&#x9;&#x9;ROW_NUMBER() Over(Partition By c.contactid Order By Case When Record2RoleIdName = 'Apoderado' Then 1 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; When Record2RoleIdName = 'Padre' Then 2&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; When Record2RoleIdName = 'Madre' Then 3&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;End Asc) As PrioridadApoderado&#xA;&#x9;&#x9;From Contact c&#xA;&#x9;&#x9;&#x9;&#x9;Left Join StringMap sm On c.GENDERCODE = sm.ATTRIBUTEVALUE And sm.ATTRIBUTENAME = 'gendercode' &#xA;&#x9;&#x9;&#x9;&#x9;Left Join Connection cn On c.contactid = cn.Record1Id And cn.Record2RoleIdName in ('Apoderado','Padre','Madre')&#xA;&#x9;&#x9;&#x9;&#x9;Left Join Contact c2 On cn.Record2Id = c2.contactid&#xA;&#x9;&#x9;where  c.ContactId in (&#xA;&quot;&#xA;+&#xA;&#xA; @[User::v_ListaPersonasParaCRM365] &#xA;&#xA;+&#xA;&quot;)&#xA;) x&#xA;&#xA;&#xA;Where x.PrioridadApoderado = 1&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="v_QueryPersonasParaCRM365">
      <DTS:VariableValue
        DTS:DataType="8">
Select x.ContactId, x.FirstName, x.MiddleName, x.LastName, x.BirthDate, x.Genero, 
		x.CRM_TipoDocumentoName, x.CRM_NroDocumento, x.Telephone1, x.Telephone2, x.Telephone3, 
		x.EmailAddress1, x.EmailAddress2, x.EmailAddress3, 
		x.Tipo_Apoderado, x.FullName_Apoderado, x.CRM_TipoDocumentoName_Apoderado, x.CRM_NroDocumento_Apoderado
From (
		Select c.ContactId, 
				c.FirstName, 
				c.MiddleName, 
				c.LastName, 
				c.BirthDate, 
				SUBSTRING(sm.VALUE,1,50) As Genero,
				c.CRM_TIPODOCUMENTONAME, 
				c.CRM_NRODOCUMENTO, 
				c.TELEPHONE1, 
				c.TELEPHONE2, 
				c.TELEPHONE3,
				c.EMAILADDRESS1, 
				c.EMAILADDRESS2, 
				c.EMAILADDRESS3,
				cn.Record2RoleIdName As Tipo_Apoderado,
				SUBSTRING(cn.Record2IdName,1,500) As FullName_Apoderado,
				c2.CRM_TIPODOCUMENTONAME As CRM_TIPODOCUMENTONAME_APODERADO,
				c2.CRM_NRODOCUMENTO As CRM_NRODOCUMENTO_APODERADO,
				ROW_NUMBER() Over(Partition By c.contactid Order By Case When Record2RoleIdName = 'Apoderado' Then 1 
																		 When Record2RoleIdName = 'Padre' Then 2
																		 When Record2RoleIdName = 'Madre' Then 3
																	End Asc) As PrioridadApoderado
		From Contact c
				Left Join StringMap sm On c.GENDERCODE = sm.ATTRIBUTEVALUE And sm.ATTRIBUTENAME = 'gendercode' 
				Left Join Connection cn On c.contactid = cn.Record1Id And cn.Record2RoleIdName in ('Apoderado','Padre','Madre')
				Left Join Contact c2 On cn.Record2Id = c2.contactid
		where  c.ContactId in (
)
) x


Where x.PrioridadApoderado = 1</DTS:VariableValue>
    </DTS:Variable>
