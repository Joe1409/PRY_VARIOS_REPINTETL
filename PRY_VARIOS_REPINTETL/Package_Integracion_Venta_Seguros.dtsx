    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{B05A2308-BAAC-4E23-9B35-762A925037A7}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;&#xA;        SELECT A.FECHA_CREACION AS FEC_CREACION,&#xA;               A.COD_LINEA_NEGOCIO,&#xA;               A.GUID_CRM_ALUM,&#xA;               A.SEC_ALUM AS SEC_ALUMNO,&#xA;               T.COD_ALUMNO,&#xA;               ME.NOMBRE AS DES_MODALIDAD_ESTUDIO,&#xA;               T.COD_PRODUC_MAT AS COD_PRODUCTO,&#xA;               PR.DESCRIPCION AS DES_CARRERA, &#xA;               T.CICLO_ALUMNO AS NUM_CICLO_ALUMNO,&#xA;               L.DESCRIPCION AS DES_LOCAL,&#xA;               RE.EMAIL_PRINCIPAL AS DES_EMAIL_RESP_ECONOMICO,&#xA;               RE.CELULAR AS DES_CELULAR_RESP_ECONOMICO,&#xA;               ROW_NUMBER() OVER (PARTITION BY A.GUID_CRM_ALUM, A.SEC_ALUM ORDER BY T.ID DESC) AS MAXIMA_MAT&#xA;            FROM WSACTDATOS.AD_ALUMNO            A,&#xA;                 MATRICULA@PRODSI2               T,&#xA;                 MODALIDAD_ESTUD@PRODSI2         ME,&#xA;                 PRODUCTO@PRODSI2                PR,&#xA;                 LOCAL@PRODSI2                   L,&#xA;                 WSACTDATOS.AD_RESP_ECONOMICO   RE         &#xA;           WHERE A.COD_LINEA_NEGOCIO = 'E' &#xA;             AND SUBSTR(A.USUARIO_CREACION,2,10) = T.COD_ALUMNO&#xA;             AND T.COD_LINEA_NEGOCIO = PR.COD_LINEA_NEGOCIO&#xA;             AND T.COD_PRODUC_MAT = PR.COD_PRODUCTO&#xA;             AND PR.ES_CARRERA = 'SI'&#xA;             AND T.COD_LINEA_NEGOCIO = ME.COD_LINEA_NEGOCIO(+)&#xA;             AND T.COD_MODAL_EST = ME.COD_MODAL_EST(+)                  &#xA;             AND T.COD_LOCAL = L.COD_LOCAL(+)          &#xA;             AND T.COD_LINEA_NEGOCIO = 'E'&#xA;             AND A.GUID_CRM_ALUM = RE.GUID_CRM_ALUM(+)&#xA;             AND A.SEC_ALUM = RE.SEC_ALUM(+)&#xA;             &quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="v_SelectReporteAlumnoE">
      <DTS:VariableValue
        DTS:DataType="8">
        SELECT A.FECHA_CREACION AS FEC_CREACION,
               A.COD_LINEA_NEGOCIO,
               A.GUID_CRM_ALUM,
               A.SEC_ALUM AS SEC_ALUMNO,
               T.COD_ALUMNO,
               ME.NOMBRE AS DES_MODALIDAD_ESTUDIO,
               T.COD_PRODUC_MAT AS COD_PRODUCTO,
               PR.DESCRIPCION AS DES_CARRERA, 
               T.CICLO_ALUMNO AS NUM_CICLO_ALUMNO,
               L.DESCRIPCION AS DES_LOCAL,
               RE.EMAIL_PRINCIPAL AS DES_EMAIL_RESP_ECONOMICO,
               RE.CELULAR AS DES_CELULAR_RESP_ECONOMICO,
               ROW_NUMBER() OVER (PARTITION BY A.GUID_CRM_ALUM, A.SEC_ALUM ORDER BY T.ID DESC) AS MAXIMA_MAT
            FROM WSACTDATOS.AD_ALUMNO            A,
                 MATRICULA@PRODSI2               T,
                 MODALIDAD_ESTUD@PRODSI2         ME,
                 PRODUCTO@PRODSI2                PR,
                 LOCAL@PRODSI2                   L,
                 WSACTDATOS.AD_RESP_ECONOMICO   RE         
           WHERE A.COD_LINEA_NEGOCIO = 'E' 
             AND SUBSTR(A.USUARIO_CREACION,2,10) = T.COD_ALUMNO
             AND T.COD_LINEA_NEGOCIO = PR.COD_LINEA_NEGOCIO
             AND T.COD_PRODUC_MAT = PR.COD_PRODUCTO
             AND PR.ES_CARRERA = 'SI'
             AND T.COD_LINEA_NEGOCIO = ME.COD_LINEA_NEGOCIO(+)
             AND T.COD_MODAL_EST = ME.COD_MODAL_EST(+)                  
             AND T.COD_LOCAL = L.COD_LOCAL(+)          
             AND T.COD_LINEA_NEGOCIO = 'E'
             AND A.GUID_CRM_ALUM = RE.GUID_CRM_ALUM(+)
             AND A.SEC_ALUM = RE.SEC_ALUM(+)
             </DTS:VariableValue>
    </DTS:Variable>
